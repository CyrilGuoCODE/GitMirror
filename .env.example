# 服务器配置
PORT=3001
NODE_ENV=development

# 日志配置
LOG_LEVEL=info
LOG_RETENTION_DAYS=30

# 同步配置
AUTO_SYNC=true
SYNC_INTERVAL=3600  # 单位：秒
CONFLICT_STRATEGY=prefer_source  # prefer_source, prefer_destination, manual

# 工作目录和配置文件路径（可选）
# WORK_DIR=./repos
# CONFIG_FILE_PATH=./.gitmirror.yml

# 各Git平台的访问令牌
GITHUB_TOKEN=your_github_token
GITEE_TOKEN=your_gitee_token
GITLAB_TOKEN=your_gitlab_token
GITCODE_TOKEN=your_gitcode_token 